{
  "cli": {
    "version": ">= 15.0.12",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "developmentClient": false,
      "autoIncrement": true,
      "android": {
        "buildType": "apk"
      },
      "env": {
        "SERVER_ROUTE": "https://task-flow-production.up.railway.app"
      }
    },
    "production": {
      "developmentClient": false,
      "distribution": "internal",
      "autoIncrement": true,
      "android": {
        "buildType": "apk"
      },
      "env": {
        "SERVER_ROUTE": "https://task-flow-production.up.railway.app"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
